    "/notices": {
      "post": {
        "tags": ["Notices"],
        "summary": "Get notices by category",
        "parameters": [],

        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NoticesRequest"
              }
