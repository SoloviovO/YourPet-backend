          "birthday": {
            "type": "string",
            "pattern": "^(/^\\d{2}\\.\\d{2}\\.\\d{4}$/)$"
          },
          "phone": {
            "type": "string",
            "pattern": "^(/^\\+380\\d{9}$/)$"
          },
          "city": {
            "type": "string",
            "pattern": "^(/^[A-Z][a-z]+(?:[\\s-][A-Z][a-z]+)*$/)$"
          },
          "image": {
            "type": "string",
            "format": "binary"
