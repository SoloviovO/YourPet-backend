        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NoticesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid category, please select one of next category: sell; lost/found; in good hands",
            "content": {}
          }
        }
      }
    }
